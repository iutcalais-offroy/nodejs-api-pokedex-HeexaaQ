meta {
  name: Get All Cards
  type: http
  seq: 1
}

get {
  url: {{localhost}}/api/cards
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

vars:pre-request {
  localhost: http://localhost:3001
}

docs {
  Get all available cards in the game.
  
  Requires authentication.
  
  Returns:
  - 200: List of all cards
  - 401: Unauthorized (missing or invalid token)
}
